<?xml version="1.0"?>
<ruleList>
    <emulator name="LIME3DS">
        <!-- Nintendo 3DS emulator Lime3DS -->
        <rule type="androidpackage">
            <entry>io.github.lime3ds.android/.activities.EmulationActivity</entry>
            <entry>io.github.lime3ds.android/.ui.main.MainActivity</entry>
        </rule>
    </emulator>
    <emulator name="SKYLINE">
        <!-- Nintendo Switch emulator Skyline -->
        <rule type="androidpackage">
            <entry>skyline.emu/emu.skyline.EmulationActivity</entry>
        </rule>
    </emulator>
    <emulator name="SUYU">
        <!-- Nintendo Switch emulator Suyu -->
        <rule type="androidpackage">
            <entry>org.suyu.suyu_emu/org.suyu.suyu_emu.activities.EmulationActivity</entry>
        </rule>
    </emulator>
    <emulator name="SUDACHI">
        <!-- Nintendo Switch emulator Sudachi -->
        <rule type="androidpackage">
            <entry>org.sudachi.sudachi_emu.ea/org.sudachi.sudachi_emu.activities.EmulationActivity</entry>
			<entry>org.sudachi.sudachi_emu.ea/org.sudachi.sudachi_emu.ui.main.MainActivityy</entry>
        </rule>
    </emulator>	
    <emulator name="YUZU">
        <!-- Nintendo Switch emulator Yuzu -->
        <rule type="androidpackage">
            <entry>org.yuzu.yuzu_emu/org.yuzu.yuzu_emu.activities.EmulationActivity</entry>
        </rule>
    </emulator>
    <emulator name="YUZU-EA">
        <!-- Nintendo Switch emulator Yuzu (Early Access) -->
        <rule type="androidpackage">
            <entry>org.yuzu.yuzu_emu.ea/org.yuzu.yuzu_emu.activities.EmulationActivity</entry>
        </rule>
    </emulator>	
</ruleList>